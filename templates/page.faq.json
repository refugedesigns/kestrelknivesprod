        "faq_item_QUBntp": {
          "type": "faq_item",
          "settings": {
            "question": "Why are most knives sold out?",
            "answer": "We build knives in limited runs to maintain precision and quality. Some models sell out fast, but we’re working toward keeping core models available more consistently. If there’s a knife you’ve been waiting on, join our email list to be notified as soon as it’s back in stock.",
            "category": "Ordering & Availability"
          }
        },
        "faq_item_dFnxeH": {
          "type": "faq_item",
          "settings": {
            "question": "Will you be bringing back old models?",
            "answer": "Yes. We’re currently ramping up production to bring many of your favorite models and sets back. The goal is to honor the legacy of what made Kestrel great while pushing the craftsmanship even further.",
            "category": "Ordering & Availability"
          }
        },
        "faq_item_ggEPQG": {
          "type": "faq_item",
          "settings": {
            "question": "When is the next sale?",
            "answer": "We don’t run standard sales anymore. We price our knives fairly all year long, but if you’re in a unique situation or a longtime customer, reach out to us. We may be able to help with a discount.",
            "category": "Ordering & Availability"
          }
        },
        "faq_item_gVttxV": {
          "type": "faq_item",
          "settings": {
            "question": "Do you ship internationally?",
            "answer": "Yes. We ship worldwide for the most part. For international orders, email sales@kestrelknives.com for a shipping quote. Please note that duties, VAT, and import taxes are the buyer’s responsibility and vary by country.",
            "category": "Shipping"
          }
        },
