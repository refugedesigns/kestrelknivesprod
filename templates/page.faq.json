          "settings": {
            "question": "Will I receive a tracking number?",
            "answer": "Yes. You’ll receive a confirmation email right after ordering, and another with tracking details once your package ships.",
            "category": "Shipping"
          }
        },
        "faq_item_yNLRDz": {
          "type": "faq_item",
          "settings": {
            "question": "Domestic shipments",
